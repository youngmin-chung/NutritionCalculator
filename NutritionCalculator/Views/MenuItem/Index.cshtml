@model MenuItemViewModel
@{
    ViewData["Title"] = "Index";
}
<h4>Items In Category - @Model.CategoryName</h4>
<ul class="list-group col-sm-8 col-xs-12">
    @{
        foreach (MenuItem item in Model.MenuItems)
        {
            <li class="list-group-item">
                @item.Description
            </li>
        }
    }
</ul>